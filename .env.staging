# ENVIRONMENT VARIABLES FOR STAGING
NODE_ENV=staging

# Connection Pooling via PgBouncer
DATABASE_URL="postgresql://user:pass@staging-db.com/grubhub?pgbouncer=true"

PORT=8080
FRONTEND_URL=https://staging.grubhub.com
REDIS_URL=redis://staging-redis-url:6379

NEXT_PUBLIC_API_URL=https://staging-api.grubhub.com

# SECRETS (Do NOT prefix with NEXT_PUBLIC_)
RAZORPAY_KEY_ID=staging_key_here
RAZORPAY_KEY_SECRET=staging_secret_here
RAZORPAY_WEBHOOK_SECRET=staging_webhook_secret_here
