# ENVIRONMENT VARIABLES FOR PRODUCTION
NODE_ENV=production

# Connection Pooling via PgBouncer (Required for Serverless)
DATABASE_URL="postgresql://user:pass@prod-db.com/grubhub?pgbouncer=true"

PORT=8080
FRONTEND_URL=https://www.grubhub.com
REDIS_URL=redis://prod-redis-url:6379

NEXT_PUBLIC_API_URL=https://api.grubhub.com

# SECRETS (Do NOT prefix with NEXT_PUBLIC_)
RAZORPAY_KEY_ID=prod_key_here
RAZORPAY_KEY_SECRET=prod_secret_here
RAZORPAY_WEBHOOK_SECRET=prod_webhook_secret_here
